{% macro active(route) %}
    {% if app.current_route starts with route%} class="active open"{% endif %}
{% endmacro %}

{% macro link(text, target) %}
    {% if target == app.current_route %}
    <li class="active"><a href="{{ path(target) }}">{{ text|trans }}</a></li>
    {% else %}
    <li><a href="{{ path(target) }}">{{ text }}</a></li>
    {% endif %}
{% endmacro %}
