{
    "name": "karlito15/marvel-blurp",
    "homepage": "https://github.com/Karlito15/marvel-blurp",
    "description": "Theme for Marvels Website Symfony",
    "keywords": ["php", "apache", "symfony", "composer", "bootstrap", "font-awesome"],
    "authors": [
        {
            "name": "Giancarlo PALUMBO",
            "email": "giancarlo.palumbo@free.fr",
            "homepage": "https://giancarlo-palumbo.ovh/",
            "role": "Freelance"
        }
    ],
    "type": "symfony-bundle",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "composer/package-versions-deprecated": "1.11.99.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^7.4.3"
    },
    "autoload": {
        "psr-4": {
            "KarlitoWeb\\Blurp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KarlitoWeb\\Blurp\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.29"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "vendor-dir": "vendor"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.*"
        }
    }
}
